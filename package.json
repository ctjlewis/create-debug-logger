{
  "name": "create-debug-logger",
  "author": {
    "name": "C. Lewis",
    "email": "ctj.lewis@icloud.com"
  },
  "type": "module",
  "version": "1.2.0",
  "description": "Simple debug logging.",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "ava": "^4.0.1",
    "eslint": "^8.8.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "build": "tsm build",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "test": "ava",
    "lint": "eslint src --fix"
  },
  "dependencies": {
    "chalk": "^5.0.0",
    "console": "^0.7.2",
    "node-fetch": "^3.2.0",
    "socket.io": "^4.4.0"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--no-warnings",
      "--loader=@tsmodule/tsm"
    ]
  },
  "peerDependencies": {
    "@tsmodule/tsm": "^7.1.0"
  }
}
